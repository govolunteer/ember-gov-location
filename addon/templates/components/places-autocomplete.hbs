<input
  value={{value}}
  id="place-input"
  class={{inputClass}}
  placeholder={{placeholder}}
  tabindex={{tabindex}}
  disabled={{disabled}}
  onclick={{action "update" value="target.value"}}
  onBlur={{action "onFocusOut"}}
  onFocusOut={{action "onFocusOut"}}
  enter={{action "update" value="target.value"}}
>
