<input
  value={{value}}
  id="place-input"
  class="c-places-autocomplete__input {{inputClass}}"
  placeholder={{placeholder}}
  tabindex={{tabindex}}
  disabled={{disabled}}
  onclick={{action (mut value) target.value}}
  onBlur={{action "onFocusOut"}}
  onFocusOut={{action "onFocusOut"}}
  enter={{action (mut value) target.value}}
>
