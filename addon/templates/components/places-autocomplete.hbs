{{input
  class=(concat "c-places-autocomplete__input" inputClass)
  disabled=(if inputDisabled "disabled")
  enter=(action (mut value) target.value)
  id=this.inputId
  onclick=(action (mut value) target.value)
  focus-out=(action "checkClearField")
  placeholder=this.placeholder
  tabindex=this.inputTabindex
  value=value
}}
